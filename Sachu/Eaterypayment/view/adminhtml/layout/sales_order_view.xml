<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="order_tab_info">
            <action method="setTemplate">
                <argument name="template" translate="true" xsi:type="string">Sachu_Eaterypayment::order/view/tab/info.phtml</argument>
            </action>
            <block class="Magento\Framework\View\Element\Template"
                   name="custom_payment_shipping_block"
                   template="Sachu_Eaterypayment::order/view/custom_payment_shipping.phtml"
                   after="shipping_and_tracking"/>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="custom_order_js" template="Sachu_Eaterypayment::js.phtml"/>
        </referenceContainer>
    </body>
</page>
